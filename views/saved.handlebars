<div>
    <h1 class="card z-depth-3 center-align">Saved Articles</h1>
    <div class="row">
        <div class="col s8">
            {{#each article}}
            {{#if saved}}
            <div class="card-panel">
                <a href="{{this.link}}" id="btnSignUp">
                    <h4>{{this.title}}</h4>
                </a>
                <h5>{{this.summary}}</h5>
                <button class="writeNote btn btn-success" data-id="{{this.id}}">Write Note</button>
            </div>
            {{/if}}
            {{/each}}
        </div>
    </div>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="/js/app.js"></script>