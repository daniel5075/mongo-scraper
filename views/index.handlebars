<div>
  <h1 class="card z-depth-3 center-align">Read the lates US news from the Wall Steet Journal</h1>

  <div class="row">
    {{!-- <div class="col s2"></div> --}}
    <div class="col s8">
      {{#each article}}
      <div class="card-panel">
        <a href="{{this.link}}" id="btnSignUp">
          <h4>{{this.title}}</h4>
        </a>
        <h5>{{this.summary}}</h5>
        <button type="button" class="btn btn-success" id="saveArticle" data-id="{{this.id}}" data-saved="{{saved}}">Save
          Article</button>
      </div>
      {{/each}}
    </div>
  </div>